{
	"name": "@ctx-core/isomorphic-fetch",
	"version": "6.0.6",
	"description": "isomorphic-fetch & @types/isomorphic-fetch",
	"keywords": [
		"ctx-core",
		"fetch"
	],
	"homepage": "https://github.com/ctx-core/isomorphic-fetch#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/isomorphic-fetch/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/isomorphic-fetch/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		"require": "./dist/index.cjs",
		"import": "./dist/index.js"
	},
	"browser": "./dist/browser.js",
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"require = require('esm')(module)\nmodule.exports = require('./index.js')\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/dom": "workspace:^11.0.8",
		"esm": "^3.2.25",
		"node-fetch": "^2.6.1"
	},
	"devDependencies": {
		"@types/node": "^16.3.2",
		"@types/node-fetch": "^2.5.11",
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}
